{
  "name": "feriados_api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "10.19.0",
    "yarn": "1.22.4"
  },
  "scripts": {
    "dist": "sucrase ./src -d /dist --transforms javascript,imports",
    "dev": "nodemon src/server.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "test": "jest",
    "sequelize:drop": "sequelize db:migrate:undo:all",
    "sequelize:prod": "sequelize db:migrate sequelize db:seed:all",
    "start": "node ./dist/server.js",
    "start:prod": "yarn install && yarn sequelize:prod && yarn dist && yarn start"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-load-sync": "^1.0.0",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "sucrase": "^3.15.0"
  }
}
